{
    "name": "landslidecreative/ls-boilerplate-wp",
    "authors": [
        {
            "name": "Landslide Creative",
            "email": "info@landslidecreative.com"
        }
    ],
    "repositories" : [
          {
               "type" : "composer",
               "url"  : "https://wpackagist.org"
          },
          {
               "type" : "vcs",
               "url" : "git@github.com:jsjenkins/advanced-custom-fields-pro.git"
          },
          {
               "type" : "vcs",
               "url" : "git@github.com:jsjenkins/gravityforms.git"
          },
          {
               "type" : "vcs",
               "url" : "git@github.com:jsjenkins/wp-rocket.git"
          },
          {
               "type" : "vcs",
               "url" : "git@github.com:jsjenkins/wp-migrate-db-pro.git"
          },
          {
               "type" : "vcs",
               "url" : "git@github.com:jsjenkins/events-calendar-pro.git"
          }
     ],
     "extra" : {
          "installer-paths" : {
               "wp-content/plugins/{$name}/" : ["type:wordpress-plugin"],
               "wp-content/themes/{$name}/" : ["type:wordpress-theme"]
          }
     },
    "minimum-stability": "dev",
    "prefer-stable" : true,
    "require": {
          "landslidecreative/advanced-custom-fields-pro" : "6.1.7",
          "wpackagist-plugin/akismet": "5.2",
          "wpackagist-plugin/classic-editor" : "1.6.3",
          "wpackagist-plugin/duplicate-post" : "4.5",
          "landslidecreative/gravityforms" : "2.7.10",
          "wpackagist-plugin/imagify" : "2.1.1",
          "wpackagist-plugin/intuitive-custom-post-order" : "3.1.4.1",
          "wpackagist-plugin/query-monitor" : "3.13.1",
          "wpackagist-plugin/safe-redirect-manager" : "2.0.1",
          "wpackagist-plugin/svg-support" : "2.5.5",
          "wpackagist-plugin/the-events-calendar" : "6.0.3.1",
          "landslidecreative/events-calendar-pro" : "6.0.2",
          "wpackagist-plugin/user-switching" : "1.7.0",
          "wpackagist-plugin/wordfence" : "7.10.2",
          "wpackagist-plugin/wp-crontrol" : "1.15.3",
          "landslidecreative/wp-migrate-db-pro" : "2.6.9",
          "landslidecreative/wp-rocket" : "3.14.2",
          "wpackagist-plugin/wordpress-seo" : "20.11"
    },
    "config": {
        "allow-plugins": {
            "composer/installers": true
        }
    }
}
